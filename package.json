{
  "name": "pdfassembler",
  "version": "0.1.1",
  "description": "The missing piece to edit PDF files directly in the browser. Disassembles PDFs to edtable JavaScript objects, then assembles them back into PDF files.",
  "author": "David Schnell-Davis",
  "license": "MIT",
  "homepage": "https://www.pdfcircus.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesramsay/pdfassembler.git"
  },
  "bugs": {
    "url": "https://github.com/jamesramsay/pdfassembler/issues"
  },
  "keywords": [
    "pdf",
    "edit",
    "modify",
    "create",
    "pdf.js"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.cjs",
  "typings": "dist/index.d.ts",
  "module": "dist/psdassembler.module.js",
  "upkg": "dist/pdfassembler.umd.js",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "microbundle",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^10.5.6",
    "@types/pako": "^1.0.0",
    "@types/pdfjs-dist": "^2.7.4",
    "core-js": "^3.15.1",
    "es6-promise": "^4.2.8",
    "microbundle": "^0.13.3",
    "p-queue": "^6.6.2",
    "pako": "^1.0.6",
    "pdfjs-dist": "^2.8.335",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
